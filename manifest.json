{
    "manifest_version": 3,
    "name": "GeoGuesser Hacker",
    "version": "0.2",
    "description": "Uses ChatGPT to guess the exact location of the image on your screen. Displays exact coordinates on mini google maps emulator.",
    "permissions": [
        "activeTab",
        "storage",
        "sidePanel",
        "tabs",
        "tabCapture",
        "identity"
    ],
    "host_permissions": [
        "https://api.openai.com/*",
        "https://people.googleapis.com/*",
        "https://www.googleapis.com/*",
        "<all_urls>"
    ],
    "action": {
        "default_icon": {
            "128": "images/geolocationbot128.png",
            "64": "images/geolocationbot64.png",
            "16": "images/geolocationbot16.png"
        }
    },
    "side_panel": {
        "default_path": "sidepanel/sidepanel.html"
    },
    "oauth2": {
      "client_id": "1071340287003-abdh20jri6ohcderbq6hbdgh4icrvq18.apps.googleusercontent.com",
      "scopes" :[
        "https://www.googleapis.com/auth/userinfo.profile",
        "https://www.googleapis.com/auth/userinfo.email"
      ]
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "key":""
 
  }